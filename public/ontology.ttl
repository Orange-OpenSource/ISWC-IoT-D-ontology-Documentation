@prefix : <http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#> .

<http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD> rdf:type owl:Ontology ;
                                                         owl:imports <http://elite.polito.it/ontologies/eupont.owl#> ,
                                                                     <https://saref.etsi.org/core/> ,
                                                                     <https://www.w3.org/2019/wot/td#> ;
                                                         <http://purl.org/dc/terms/description> "IoT-D reprensents IoT systems context data that allows inference of dependencies between IoT systems." ;
                                                         <http://purl.org/dc/terms/title> "IoT dependencies ontology" ;
                                                         <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD" ;
                                                         owl:versionIRI "http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD/0.0.4" ;
                                                         owl:versionInfo "0.0.4" .

