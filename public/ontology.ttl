#################################################################
#    Object Properties
#################################################################

###  http://www.data-knowledge.org/dk/flowsFrom
<http://www.data-knowledge.org/dk/flowsFrom> rdf:type owl:ObjectProperty ;
                                             owl:equivalentProperty :flowsFrom ;
                                             rdfs:domain <http://www.data-knowledge.org/dk/DataFlow> ;
                                             rdfs:range :Action ;
                                             <http://purl.org/vocab/vann/example> "Master data repository sends out dataset 324"@en ,
                                                                                  "Stammdatenspeicher sendet Datensatz 324"@de ;
                                             rdfs:comment "Interface A sends data"@en ,
                                                          "Schnittstelle A sendet Daten"@de ;
                                             rdfs:isDefinedBy <http://www.data-knowledge.org/dk/> ;
                                             rdfs:label "fließt von"@de ,
                                                        "flows from"@en .


###  http://www.data-knowledge.org/dk/flowsTo
<http://www.data-knowledge.org/dk/flowsTo> rdf:type owl:ObjectProperty ;
                                           owl:equivalentProperty :flowsTo ;
                                           rdfs:domain <http://www.data-knowledge.org/dk/DataFlow> ;
                                           rdfs:range :Action ;
                                           <http://purl.org/vocab/vann/example> "Dataset 324 is sent to ERP system"@en ,
                                                                                "Datensatz 324 wird an ERP-System gesendet"@de ;
                                           rdfs:comment "Data is sent to Interface B"@en ,
                                                        "Daten werden zu Schnittstelle B gesendet"@de ;
                                           rdfs:isDefinedBy <http://www.data-knowledge.org/dk/> ;
                                           rdfs:label "fließt an"@de ,
                                                      "flows to"@en .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#allowsAction
:allowsAction rdf:type owl:ObjectProperty ;
              rdfs:domain :Service ;
              rdfs:range :Action ;
              rdfs:comment "A relationship between a service and an action, a service call allow action execution." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#consumesService
:consumesService rdf:type owl:ObjectProperty ;
                 rdfs:domain :Device ;
                 rdfs:range :Service .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#controls
:controls rdf:type owl:ObjectProperty ;
          rdfs:domain :Actuator ;
          rdfs:range :PropertyOfInterest .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#flowsFrom
:flowsFrom rdf:type owl:ObjectProperty ;
           rdfs:domain :DataFlow ;
           rdfs:range :Action .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#flowsTo
:flowsTo rdf:type owl:ObjectProperty ;
         rdfs:domain :DataFlow ;
         rdfs:range :Action .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasAction
:hasAction rdf:type owl:ObjectProperty ;
           rdfs:domain :Rule ;
           rdfs:range :Action .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasApplicationAction
:hasApplicationAction rdf:type owl:ObjectProperty ;
                      rdfs:domain :Application ;
                      rdfs:range <http://elite.polito.it/ontologies/eupont.owl#Action> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasChildTrigger
:hasChildTrigger rdf:type owl:ObjectProperty ;
                 rdfs:domain :ComplexTrigger ;
                 rdfs:range :Trigger .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasConnectivityDependency
:hasConnectivityDependency rdf:type owl:ObjectProperty ;
                           rdfs:domain <https://saref.etsi.org/core/Device> ;
                           rdfs:range <https://saref.etsi.org/core/Device> ;
                           rdfs:comment "Denotes connectivity dependency relationship between two devices." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasEnvironmentDependencyTo
:hasEnvironmentDependencyTo rdf:type owl:ObjectProperty ;
                            rdfs:domain <https://saref.etsi.org/core/Device> ;
                            rdfs:range <https://saref.etsi.org/core/Device> ;
                            rdfs:comment "Denotes environment dependency relationship between two devices." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasImplicitServiceDependencyTo
:hasImplicitServiceDependencyTo rdf:type owl:ObjectProperty ;
                                rdfs:domain :Device ;
                                rdfs:range :Device ;
                                rdfs:comment "Denotes implicit service dependency between two IoT devices." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasPropertyAffordance
:hasPropertyAffordance rdf:type owl:ObjectProperty ;
                       rdfs:domain :Application ,
                                   <https://saref.etsi.org/core/Device> ;
                       rdfs:range <https://www.w3.org/2019/wot/td#PropertyAffordance> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasRule
:hasRule rdf:type owl:ObjectProperty ;
         rdfs:domain :Application ;
         rdfs:range <http://elite.polito.it/ontologies/eupont.owl#Rule> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasServiceDependencyTo
:hasServiceDependencyTo rdf:type owl:ObjectProperty ;
                        rdfs:domain <https://saref.etsi.org/core/Device> ;
                        rdfs:range <https://saref.etsi.org/core/Device> ;
                        rdfs:comment "Denotes direct service dependency relationship between two devices." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasState
:hasState rdf:type owl:ObjectProperty ;
          rdfs:domain :Device ;
          rdfs:range :StateOfInterest .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasStateDependencyTo
:hasStateDependencyTo rdf:type owl:ObjectProperty ;
                      rdfs:domain <https://saref.etsi.org/core/Device> ;
                      rdfs:range <https://saref.etsi.org/core/Device> ;
                      rdfs:comment "Denotes state dependency relationship between two devices." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasTrigger
:hasTrigger rdf:type owl:ObjectProperty ;
            rdfs:domain :Rule ;
            rdfs:range :Trigger .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#observes
:observes rdf:type owl:ObjectProperty ;
          rdfs:domain :Sensor ;
          rdfs:range :PropertyOfInterest .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#offers
:offers rdf:type owl:ObjectProperty ;
        rdfs:domain :Device ;
        rdfs:range :Service .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Trigger ;
           rdfs:range :StateOfInterest ;
           rdfs:comment "A relationship between a trigger and state to experss that a trigger occurs at a defined state of the device." .


###  https://saref.etsi.org/core/actsUpon
<https://saref.etsi.org/core/actsUpon> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf owl:topObjectProperty .


###  https://saref.etsi.org/core/consistsOf
<https://saref.etsi.org/core/consistsOf> rdf:type owl:ObjectProperty .


###  https://saref.etsi.org/core/controlsProperty
<https://saref.etsi.org/core/controlsProperty> rdf:type owl:ObjectProperty .


###  https://saref.etsi.org/core/hasPropertyOfInterest
<https://saref.etsi.org/core/hasPropertyOfInterest> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain <https://saref.etsi.org/core/FeatureOfInterest> ;
                                                    rdfs:range <https://saref.etsi.org/core/PropertyOfInterest> ;
                                                    rdfs:comment "Links a feature of interest to one of its properties of interest."@en ;
                                                    rdfs:label "has property of interest"@en ;
                                                    <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced saref:hasPropertyOfInterest and saref:isPropertyOfInterestOf. Cannot use saref:hasProperty due to the combination of cardinality restrictions and sub-property chain axioms makes the ontology exist the OWL 2 DL prodile."@en .


###  https://saref.etsi.org/core/hasStateOfInterest
<https://saref.etsi.org/core/hasStateOfInterest> rdf:type owl:ObjectProperty ;
                                                 rdfs:domain <https://saref.etsi.org/core/FeatureOfInterest> ;
                                                 rdfs:range <https://saref.etsi.org/core/StateOfInterest> ;
                                                 rdfs:comment "Links a feature of interest to one of its states of interest."@en ;
                                                 rdfs:label "has state of interest"@en ;
                                                 <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced saref:hasStateOfInterest and saref:isStateOfInterestOf, parallel to saref:hasPropertyOfInterest and saref:isPropertyOfInterestOf."@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasDeviceName
:hasDeviceName rdf:type owl:DatatypeProperty ;
               rdfs:domain <https://saref.etsi.org/core/Device> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasDeviceTypeName
:hasDeviceTypeName rdf:type owl:DatatypeProperty ;
                   rdfs:domain <https://saref.etsi.org/core/Device> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasIPAddress
:hasIPAddress rdf:type owl:DatatypeProperty ;
              rdfs:domain <https://saref.etsi.org/core/Device> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasMacAddress
:hasMacAddress rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain <https://saref.etsi.org/core/Device> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasManufacturerName
:hasManufacturerName rdf:type owl:DatatypeProperty ;
                     rdfs:domain <https://saref.etsi.org/core/Device> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#hasServiceName
:hasServiceName rdf:type owl:DatatypeProperty ;
                rdfs:domain <https://saref.etsi.org/core/Service> .


#################################################################
#    Classes
#################################################################

###  http://elite.polito.it/ontologies/eupont.owl#Action
<http://elite.polito.it/ontologies/eupont.owl#Action> rdf:type owl:Class .


###  http://elite.polito.it/ontologies/eupont.owl#Rule
<http://elite.polito.it/ontologies/eupont.owl#Rule> rdf:type owl:Class ;
                                                    owl:equivalentClass :Rule .


###  http://elite.polito.it/ontologies/eupont.owl#Trigger
<http://elite.polito.it/ontologies/eupont.owl#Trigger> rdf:type owl:Class .


###  http://www.data-knowledge.org/dk/DataFlow
<http://www.data-knowledge.org/dk/DataFlow> rdf:type owl:Class ;
                                            rdfs:comment "Indicates the actual connection and data exchange between particular applications"@en ;
                                            rdfs:label "Data Flow"@en .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Action
:Action rdf:type owl:Class ;
        rdfs:subClassOf <http://elite.polito.it/ontologies/eupont.owl#Action> ;
        rdfs:comment "An action to perform by calling a service of an IoT device." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Actuator
:Actuator rdf:type owl:Class ;
          owl:equivalentClass <https://saref.etsi.org/core/Actuator> ;
          rdfs:subClassOf :Device ,
                          :IoTDevice .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Application
:Application rdf:type owl:Class ;
             rdfs:comment "An IoT application that compose IoT devices services to create added value/complex services, it can be a trigger-action plateform." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Broker
:Broker rdf:type owl:Class ;
        rdfs:subClassOf :Application ;
        rdfs:comment "broker that connect equipments." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#ComplexTrigger
:ComplexTrigger rdf:type owl:Class ;
                rdfs:subClassOf :Trigger .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#ConnectivityDevice
:ConnectivityDevice rdf:type owl:Class ;
                    rdfs:subClassOf :Device ;
                    rdfs:comment "A device that connectes home devices to the internet like: gateway, wifiRepeater, hub." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#ConnectivityService
:ConnectivityService rdf:type owl:Class ;
                     rdfs:subClassOf <https://saref.etsi.org/core/Service> ;
                     rdfs:comment "A service offred by a connectivityDevice." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#DataFlow
:DataFlow rdf:type owl:Class ;
          rdfs:subClassOf <http://www.data-knowledge.org/dk/DataFlow> ;
          rdfs:comment "Indicates the actual connection and data exchange between actions in one application." ;
          rdfs:label "Data Flow" .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Device
:Device rdf:type owl:Class ;
        owl:equivalentClass <https://saref.etsi.org/core/Device> ;
        rdfs:comment "A tangible object designed to accomplish a particular task. In order to accomplish this task, the device performs one or more functions. For example, a washing machine is designed to wash (task) and to accomplish this task it performs a start and stop function." ;
        rdfs:label "Device" .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#FeatureOfInterest
:FeatureOfInterest rdf:type owl:Class ;
                   owl:equivalentClass <https://saref.etsi.org/core/FeatureOfInterest> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#IoTDevice
:IoTDevice rdf:type owl:Class ;
           rdfs:subClassOf :Device ;
           rdfs:comment "A device that is connected to the internet to share data it can be ether a sensor/actuator/processor or compositition." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Processor
:Processor rdf:type owl:Class ;
           rdfs:subClassOf :Device ;
           rdfs:comment "A device that has the capability to perform computation operations on data." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#PropertyOfInterest
:PropertyOfInterest rdf:type owl:Class ;
                    owl:equivalentClass <https://saref.etsi.org/core/PropertyOfInterest> ;
                    rdfs:comment "A quality of a feature of interest that can be measured; an aspect of a feature of interest that is intrinsic to and cannot exist without the feature" .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Rule
:Rule rdf:type owl:Class .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Sensor
:Sensor rdf:type owl:Class ;
        owl:equivalentClass <https://saref.etsi.org/core/Sensor> ;
        rdfs:subClassOf :Device ,
                        :IoTDevice .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Service
:Service rdf:type owl:Class ;
         owl:equivalentClass <https://saref.etsi.org/core/Service> .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Smokesensor
:Smokesensor rdf:type owl:Class ;
             owl:equivalentClass <https://saref.etsi.org/core/SmokeSensor> ;
             rdfs:subClassOf :Sensor .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#StateOfInterest
:StateOfInterest rdf:type owl:Class ;
                 rdfs:comment "The state in which a device can be found, e.g, ON/OFF/STANDBY, or ONLINE/OFFLINE. We propose here a list of states that are relevant for the purpose of SAREF, but this list can be extended." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Switch
:Switch rdf:type owl:Class ;
        rdfs:subClassOf :Actuator .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Temperaturesensor
:Temperaturesensor rdf:type owl:Class ;
                   owl:equivalentClass <https://saref.etsi.org/core/TemperatureSensor> ;
                   rdfs:subClassOf :Sensor .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#Trigger
:Trigger rdf:type owl:Class ;
         rdfs:subClassOf <http://elite.polito.it/ontologies/eupont.owl#Trigger> ;
         rdfs:comment "An event to react to. It is launched when the state of an IoT device changes." .


###  http://www.semanticweb.org/Lab/ontologies/2021/9/IoTD#triggerActionPlateform
:triggerActionPlateform rdf:type owl:Class ;
                        rdfs:subClassOf :Application ;
                        rdfs:comment "a kind of IoT application that is composed of rules created by end user." .


###  https://saref.etsi.org/core/Actuator
<https://saref.etsi.org/core/Actuator> rdf:type owl:Class .


###  https://saref.etsi.org/core/Device
<https://saref.etsi.org/core/Device> rdf:type owl:Class .


###  https://saref.etsi.org/core/FeatureOfInterest
<https://saref.etsi.org/core/FeatureOfInterest> rdf:type owl:Class ;
                                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <https://saref.etsi.org/core/consistsOf> ;
                                                                  owl:allValuesFrom <https://saref.etsi.org/core/FeatureOfInterest>
                                                                ] ;
                                                rdfs:comment "A feature of interest represents any real world entity from which a property or a state may be acted upon, such as observed and controlled. An instance of saref:FeatureOfInterest represents one specific real world entity."@en ;
                                                rdfs:label "Feature of interest"@en .


###  https://saref.etsi.org/core/PropertyOfInterest
<https://saref.etsi.org/core/PropertyOfInterest> rdf:type owl:Class ;
                                                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <https://saref.etsi.org/core/hasPropertyOfInterest> ;
                                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                 ] ;
                                                 rdfs:comment "Identifiable qualities of features of interest that can be acted upon by devices, such as observed or controlled. An instance of saref:PropertyOfInterest is specific to a feature of interest. It is inherent to and cannot exist without that feature of interest."@en ;
                                                 rdfs:label "Property of Interest"@en ;
                                                 <http://www.w3.org/2004/02/skos/core#example> """The air temperature of the atmosphere sample at a certain location and altitude, the received signal strength indicator of an wireless IoT connection, the luminance of the ETSI ATHENA amphitheatre.

<etsi_premises/athena#luminance> a saref:PropertyOfInterest ;
    saref:isPropertyOfInterestOf <etsi_premises/athena> ;
    saref:hasPropertyKind saref:Luminance ;
    rdfs:comment \"The luminance of amphitheatre ATHENA\"@en .
"""@en ;
                                                 <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced saref:PropertyOfInterest to solved the ambiguity between generic vs specific properties."@en ;
                                                 <http://www.w3.org/2004/02/skos/core#note> "A property of interest is the property of (OP saref:isPropertyOfInterestOf) exactly one feature of interest."@en ,
                                                                                            "Per convention, the IRI of properties of interest should consist of the IRI of their feature of interest, suffixed with a fragment identifier #{P} where {P} refers to the name of the property."@en ,
                                                                                            "Properties of interest need not always be explicited. It depends on the use case. Typically, properties of interest are useful in applications, where the association between a feature of interest and a property (i.e., the property of interest) needs to be identified and related to other properties of interest."@en .


###  https://saref.etsi.org/core/Sensor
<https://saref.etsi.org/core/Sensor> rdf:type owl:Class .


###  https://saref.etsi.org/core/Service
<https://saref.etsi.org/core/Service> rdf:type owl:Class .


###  https://saref.etsi.org/core/SmokeSensor
<https://saref.etsi.org/core/SmokeSensor> rdf:type owl:Class .


###  https://saref.etsi.org/core/StateOfInterest
<https://saref.etsi.org/core/StateOfInterest> rdf:type owl:Class ;
                                              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                owl:onProperty <https://saref.etsi.org/core/hasStateOfInterest> ;
                                                                owl:cardinality "1"^^xsd:nonNegativeInteger
                                                              ] ;
                                              rdfs:comment "Identifiable conditions that features of interest are or may be in, and that can be acted upon by devices, such as observed and controlled. An instance of saref:StateOfInterest is specific to a feature of interest. It is inherent to and cannot exist without that feature of interest."@en ;
                                              rdfs:label "State of Interest"@en ;
                                              <http://www.w3.org/2004/02/skos/core#historyNote> "V3.2.1: Introduced saref:StateOfInterest, parallel to saref:PropertyOfInterest."@en ;
                                              <http://www.w3.org/2004/02/skos/core#note> "A state of interest is the state of (OP saref:isStateOfInterestOf) exactly one feature of interest."@en ,
                                                                                         "Per convention, the IRI of states of interest should consist of the IRI of their feature of interest, suffixed with a fragment identifier #{S} where {S} refers to the name of the state."@en ,
                                                                                         "States of interest need not always be explicited. It depends on the use case. Typically, states of interest are useful in applications, where the association between a feature of interest and a state (i.e., the state of interest) needs to be identified and related to other states of interest."@en .


###  https://saref.etsi.org/core/TemperatureSensor
<https://saref.etsi.org/core/TemperatureSensor> rdf:type owl:Class .


###  https://www.w3.org/2019/wot/td#PropertyAffordance
<https://www.w3.org/2019/wot/td#PropertyAffordance> rdf:type owl:Class .


